{
  "compilerOptions": {
    "outDir": "./build",
    "allowJs": true,
    "sourceMap": false,
    "target": "es6",
    "module": "commonjs",
    "removeComments": true,
    "noImplicitAny": true,
    "strictPropertyInitialization": true,
    "strictNullChecks": true,
    "types": [
      "node",
      "webdriverio/sync",
      "@wdio/mocha-framework"
    ],
  },
  "include": [
    "./test/**/*.ts"
  ]
}